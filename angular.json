{
  "version": 1,
  "projects": {
    "pascal": {
      "root": "apps/pascal/",
      "sourceRoot": "apps/pascal/src",
      "projectType": "application",
      "prefix": "pascal",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/pascal",
            "index": "apps/pascal/src/index.html",
            "main": "apps/pascal/src/main.ts",
            "polyfills": "apps/pascal/src/polyfills.ts",
            "tsConfig": "apps/pascal/tsconfig.app.json",
            "assets": ["apps/pascal/src/favicon.ico", "apps/pascal/src/assets"],
            "styles": ["apps/pascal/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/pascal/src/environments/environment.ts",
                  "with": "apps/pascal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/pascal/src/environments/environment.ts",
                  "with": "apps/pascal/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pascal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pascal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pascal:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/pascal/tsconfig.app.json", "apps/pascal/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/pascal/jest.config.js",
            "passWithNoTests": true,
            "runInBand": true
          },
          "outputs": ["coverage/apps/pascal/"]
        }
      }
    },
